{
  "$schema": "../../../extension.schema.json",
  "$version": "1.0.0",
  "$name": "metadata-plugin",
  "$description": "metadata card configuration plugin - testing purpose",
  "features": {
    "content-metadata-presets": [
      {
        "id": "app.content.metadata.custom",
        "custom": [
          {
            "id": "app.content.metadata.renditionedGroup",
            "disabled": true
          },
          {
            "id": "app.content.metadata.qshareGroup",
            "disabled": true
          },
          {
            "id": "app.content.metadata.versionableGroup",
            "disabled": true
          },
          {
            "id": "app.content.metadata.copiedfromGroup",
            "disabled": true
          },
          {
            "id": "app.content.metadata.auditableGroup",
            "disabled": true
          },
          {
            "id": "app.content.metadata.thumbnailModificationGroup",
            "disabled": true
          },
          {
            "id": "app.content.metadata.contentGroup",
            "disabled": true
          }
        ]
      },
      {
        "kitten-images": {
          "custom:aspect": "*",
          "exif:exif": [ "exif:pixelXDimension", "exif:pixelYDimension"]
        }
      }
    ]
  }
}
